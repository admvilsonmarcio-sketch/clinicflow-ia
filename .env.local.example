# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Configurações do App
NEXT_PUBLIC_APP_NAME="MediFlow"
NEXT_PUBLIC_APP_VERSION="0.5.0"

# Integração ViaCEP (API Pública - não requer chave)
# URL base da API ViaCEP
NEXT_PUBLIC_VIACEP_API_URL=https://viacep.com.br/ws

# Configurações de Validação
# Habilitar validação rigorosa de CPF
NEXT_PUBLIC_ENABLE_CPF_VALIDATION=true

# Timeout para requisições de CEP (em ms)
NEXT_PUBLIC_CEP_REQUEST_TIMEOUT=5000

# OpenAI Configuration (for AI features)
OPENAI_API_KEY=your_openai_api_key

# N8N Configuration
N8N_WEBHOOK_URL=your_n8n_webhook_url
N8N_API_KEY=your_n8n_api_key

# EvolutionAPI Configuration
EVOLUTION_API_URL=your_evolution_api_url
EVOLUTION_API_KEY=your_evolution_api_key

# Google Calendar Integration
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret